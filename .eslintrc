{
  "env": {
    "es6": true,
    "node": true,
    "mocha": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 8 // or 2017
  },
  "globals": {
    "__base": false,
    "__controllers": false,
    "__helpers": false,
    "__lib": false,
    "__models": false,
    "__services": false,
    "beforeAll": false,
    "expect": false,
    "pending": false,
    "spyOn": false,
    "Promise": true
  },
  "rules": {
    "accessor-pairs": 2,
    "array-bracket-spacing": [2, "never"],
    "block-scoped-var": 2,
    "block-spacing": [1, "always"],
    "brace-style": [2, "1tbs"],
    "callback-return": 1,
    "camelcase": [1, {"properties": "never"}],
    "comma-dangle": [0, "always-multiline"],
    "comma-spacing": [2, {"before": false, "after": true}],
    "comma-style": [2, "last"],
    "computed-property-spacing": [2, "never"],
    "consistent-return": 0,
    "consistent-this": [2, "self"],
    "curly": 2,
    "dot-location": [1, "property"],
    "dot-notation": 1,
    "eol-last": 2,
    "eqeqeq": [2, "smart"],
    "func-names": 0,
    "func-style": [2, "declaration", { "allowArrowFunctions": true }],
    "global-require": 1,
    "guard-for-in": 1,
    "handle-callback-err": 1,
    "indent": [2, 2, {"SwitchCase": 1}],
    "key-spacing": [2, {"beforeColon": false, "afterColon": true}],
    "keyword-spacing": 2,
    "linebreak-style": 2,
    "max-depth": [1, 3],
    "max-len": [1, 220, 2],
    "max-nested-callbacks": [1, 4],
    "max-statements": [1, 25],
    "new-cap": [1, {"newIsCap": true, "capIsNew": false}],
    "new-parens": 1,
    "no-alert": 2,
    "no-array-constructor": 1,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-case-declarations": 2,
    "no-catch-shadow": 2,
    "no-delete-var": 1,
    "no-div-regex": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-else-return": 2,
    "no-empty": 1,
    "no-empty-character-class": 1,
    "no-empty-pattern": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 1,
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-func-assign": 2,
    "no-implicit-coercion": 2,
    "no-implied-eval": 2,
    "no-inline-comments": 0,
    "no-inner-declarations": 1,
    "no-invalid-this": 2,
    "no-irregular-whitespace": 1,
    "no-iterator": 2,
    "no-labels": 1,
    "no-lone-blocks": 1,
    "no-lonely-if": 2,
    "no-loop-func": 1,
    "no-magic-numbers": 0,
    "no-mixed-requires": 1,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 0,
    "no-multiple-empty-lines": [2, {"max": 2, "maxEOF": 1}],
    "no-native-reassign": 2,
    "no-negated-condition": 1,
    "no-negated-in-lhs": 2,
    "no-nested-ternary": 2,
    "no-new": 1,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-require": 2,
    "no-new-wrappers": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-trailing-spaces": 2,
    "no-param-reassign": 1,
    "no-path-concat": 0,
    "no-plusplus": 0,
    "no-process-env": 1,
    "no-process-exit": 1,
    "no-proto": 2,
    "no-redeclare": 1,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 1,
    "no-shadow": 1,
    "no-shadow-restricted-names": 2,
    "no-spaced-func": 2,
    "no-sparse-arrays": 2,
    "no-sync": 1,
    "no-throw-literal": 2,
    "no-undef": 1,
    "no-undef-init": 2,
    "no-undefined": 0,
    "no-unexpected-multiline": 1,
    "no-unreachable": 2,
    "no-unused-expressions": 0,
    "no-unused-vars": [1, {"vars": "all", "args": "none"}],
    "no-use-before-define": [2, "nofunc"],
    "no-useless-call": 1,
    "no-useless-concat": 1,
    "no-void": 2,
    "no-warning-comments": 0,
    "no-with": 2,
    "object-curly-spacing": [2, "never"],
    "quotes": [2, "single", "avoid-escape"],
    "quote-props": [2, "consistent-as-needed"],
    "radix": 2,
    "require-jsdoc": 1,
    "semi": [2, "always"],
    "semi-spacing": [2, {"before": false, "after": true}],
    "space-before-blocks": 2,
    "space-before-function-paren": [1, "always"],
    "space-in-parens": [2, "never"],
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "spaced-comment": [2, "always"],
    "strict": [2, "global"],
    "use-isnan": 2,
    "valid-typeof": 1,
    "vars-on-top": 0,
    "wrap-iife": [2, "inside"],
    "yoda": 2
  },
  "ecmaFeatures": {
    "blockBindings": true
  }
}
